  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@serpa-cloud/babel-plugin-transform-stylex": "^0.1.0-beta.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "flow-bin": "^0.178.0",
    "husky": "^4.3.8",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "bun-dev:client": "bun scripts/start.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "export REACT_APP_VERSION=$npm_package_version && node scripts/start.js",
    "dev:webpack": "NODE_ENV=development webpack --config config/webpack.server.js --mode=development -w",
    "dev:server": "nodemon ./server-build/index.js",
    "start": "node ./server-build/index.js",
    "build:webpack": "export REACT_APP_VERSION=$npm_package_version && NODE_ENV=production webpack --config config/webpack.server.js --mode=production",
    "build": "export REACT_APP_VERSION=$npm_package_version && node scripts/build.js",
    "test": "export CI=true && node scripts/test.js --collectCoverage",
    "relay": "relay-compiler",
    "schema": "get-graphql-schema ${npm_package_proxy}/_graphql > schema.graphql",
    "standard-release": "standard-version",
    "create-release": "standard-version && git push --follow-tags origin release",
    "release": "npm run create-release",
    "commitlint": "commitlint --from=main",
    "build:docker": "docker build --platform linux/amd64 -t ${npm_package_artifactory}${npm_package_name}:${npm_package_version} .",
    "push:docker": "docker push ${npm_package_artifactory}${npm_package_name}:${npm_package_version}",
    "deploy:docker": "npm run build:docker && npm run push:docker"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema.graphql",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "defaults",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
